generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Knowledge {
  id        String   @id @default(uuid())
  keywords  String[]
  answer    String
  category  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("knowledges")
} 
